# Environment variables for StressSense Windows Application
# Copy this file to .env and update values for your environment

# Backend Configuration
BACKEND_URL=http://localhost:8000
API_PREFIX=/api/v1

# Application Settings
CONFIG_FILE=device_config.json
LOG_LEVEL=INFO

# Service Settings
SERVICE_NAME=StressSenseService
SERVICE_DISPLAY_NAME=StressSense Stress Detection Service

# Detection Settings
CAMERA_INDEX=0
DETECTION_INTERVAL=300  # seconds between detections
FACE_QUALITY_THRESHOLD=0.6

# Data Settings
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY=5  # seconds